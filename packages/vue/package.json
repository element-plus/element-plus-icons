{
  "name": "@element-plus/icons-vue",
  "version": "2.0.0",
  "description": "Vue components of Element Plus Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue",
    "element-plus"
  ],
  "license": "MIT",
  "homepage": "https://element-plus.org/",
  "bugs": {
    "url": "https://github.com/element-plus/element-plus-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus-icons.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:generate && pnpm run build:build && pnpm run build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@element-plus/icons-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.1",
    "@pnpm/find-workspace-packages": "^4.0.9",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.36",
    "@types/prettier": "^2.6.3",
    "camelcase": "^6.3.0",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "esbuild": "^0.14.42",
    "esbuild-plugin-globals": "^0.1.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "prettier": "^2.6.2",
    "tsx": "^3.4.2",
    "typescript": "^4.7.2",
    "unplugin-vue": "^2.2.0",
    "vue": "^3.2.36",
    "vue-tsc": "^0.35.0"
  }
}
